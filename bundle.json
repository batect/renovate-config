{
  "extends": [
    "github>batect/renovate-config:default",
    ":semanticCommits",
    ":semanticCommitTypeAll(deps)",
    ":semanticCommitScopeDisabled"
  ],
  "packageRules": [
    {
      "matchPaths": [
        "+(.github/workflows/**/*)",
        "+(batect)",
        "+(batect.cmd)",
        "+(batect.yml)"
      ],
      "semanticCommitScope": "internal"
    }
  ],
  "automerge": true
}
